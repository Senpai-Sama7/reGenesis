{
  "$schema": "https://design-tokens.github.io/format/spec.json",
  "$description": "Motion tokens",
  "motion": {
    "enter": {
      "fast": {
        "$type": "duration",
        "$value": 150
      },
      "medium": {
        "$type": "duration",
        "$value": 250
      },
      "slow": {
        "$type": "duration",
        "$value": 400
      }
    },
    "exit": {
      "fast": {
        "$type": "duration",
        "$value": 100
      },
      "medium": {
        "$type": "duration",
        "$value": 200
      },
      "slow": {
        "$type": "duration",
        "$value": 350
      }
    }
  },
  "easing": {
    "standard": {
      "$type": "cubicBezier",
      "$value": [
        0.4,
        0,
        0.2,
        1
      ]
    },
    "accelerate": {
      "$type": "cubicBezier",
      "$value": [
        0.4,
        0,
        1,
        1
      ]
    },
    "decelerate": {
      "$type": "cubicBezier",
      "$value": [
        0,
        0,
        0.2,
        1
      ]
    },
    "emphasize": {
      "$type": "cubicBezier",
      "$value": [
        0.25,
        0.46,
        0.45,
        0.94
      ]
    },
    "overshoot": {
      "$type": "cubicBezier",
      "$value": [
        0.68,
        -0.55,
        0.265,
        1.55
      ]
    }
  }
}